use Test::More tests => 6;
use strict;
use warnings;
use Data::Dumper;
# the order is important
use Dancer::Test;
use GFE_Submission;
use Dancer::Plugin::Swagger;
use GFE_Submission::Definitions;
use GFE_Submission::API;


my $r_sequence_blank = dancer_response POST => '/api/v1/gfe?locus=HLA-A&sequence=';
ok(defined $r_sequence_blank->{content},"r_sequence_blank->{content} defined");
ok(defined $r_sequence_blank->{content}->{type},"r_sequence_blank->{content}->{type} defined");
ok($r_sequence_blank->{content}->{type} eq "Sequence","r_sequence_blank->{content}->{type} eq Sequence");


my $r_locus_blank = dancer_response POST => '/api/v1/gfe?locus=&';
ok(defined $r_locus_blank->{content},"r_locus_blank->{content} defined");
ok(defined $r_locus_blank->{content}->{type},"r_locus_blank->{content}->{type} defined");
ok($r_locus_blank->{content}->{type} eq "Locus","r_locus_blank->{content}->{type} eq Locus");



