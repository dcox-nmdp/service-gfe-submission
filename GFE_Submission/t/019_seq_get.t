use Test::More tests => 2;
use strict;
use warnings;

use Dancer::Test;
use GFE_Submission;
use Dancer::Plugin::Swagger;
use GFE_Submission::Definitions;
use GFE_Submission::API;

my $o_gfe          = GFE->new();
my $s_gfe          = "HLA-Aw1-1-7-20-10-32-7-1-1-1-6-1-5-3-5-1-1";
my $rh_gfe         = $o_gfe->getSequence("HLA-A",$s_gfe);
my $s_expected_seq = "";

ok(defined $$rh_gfe{sequence},"getSequence() returned");
ok($$rh_gfe{sequence} eq $s_expected_seq,"sequence matched expected");