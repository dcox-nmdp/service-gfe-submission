use Test::More tests => 17;
use strict;
use warnings;

use Data::Dumper;
use Dancer::Test;
use GFE_Submission;
use Dancer::Plugin::Swagger;
use GFE_Submission::Definitions;
use GFE_Submission::API;


my $o_gfe          = GFE->new();
my $rh_gfe         = $o_gfe->getGfe("HLA-A","");

ok(defined $$rh_gfe{gfe},"getGfe() successfully validated");
my $s_gfe = "HLA-Aw1-1-7-20-10-32-7-1-1-1-6-1-5-3-5-1-1";
ok($$rh_gfe{gfe} eq $s_gfe,"gfe matched expected");


my $o_gfe2        = GFE->new();
my $rh_gfe_loc    =  $o_gfe2->getGfe("HLA-Z","");
my $ra_logs       = defined $$rh_gfe_loc{Error}{log} ? $$rh_gfe_loc{Error}{log} : '';
ok(defined $$rh_gfe_loc{Error},"getGfe() correctly returned error");
ok(defined $$rh_gfe_loc{Error}{type},"getGfe() correctly returned error type");
ok($$rh_gfe_loc{Error}{type} eq "Locus","r_invalid_loc->{content}->{type} eq Locus");
ok(defined $ra_logs,"Error logs defined for getGfe() with invalid locus");
ok(scalar @{$ra_logs} > 0,"Error logs for getGfe() returned with invalid locus");

my $o_gfe3          = GFE->new();
my $rh_gfe_short    = $o_gfe3->getGfe("HLA-A","A");
my $ra_logs2        = defined $$rh_gfe_short{Error}{log} ? $$rh_gfe_short{Error}{log} : '';
ok(defined $$rh_gfe_short{Error},"Error defined for short sequence");
ok(defined $$rh_gfe_short{Error}{type},"Error type defined for short sequence defined");
ok($$rh_gfe_short{Error}{type} eq "Sequence","Error type eq Sequence");
ok(defined $ra_logs2,"Error logs defined for getGfe() with short seq");
ok(scalar @{$ra_logs2} > 0,"Error logs for getGfe() returned with short locus");

my $o_gfe4          = GFE->new();
my $rh_gfe_align    = $o_gfe4->getGfe("HLA-A","ZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZZ");
my $ra_logs3        = defined $$rh_gfe_short{Error}{log} ? $$rh_gfe_short{Error}{log} : '';
ok(defined $$rh_gfe_align{Error},"Error defined for short sequence");
ok(defined $$rh_gfe_align{Error}{type},"Error type defined for short sequence defined");
ok($$rh_gfe_align{Error}{type} eq "Alignment","Error type eq Alignment");
ok(defined $ra_logs3,"Error logs defined for getGfe() with invalid seq");
ok(scalar @{$ra_logs3} > 0,"Error logs for getGfe() returned with invalid seq");































